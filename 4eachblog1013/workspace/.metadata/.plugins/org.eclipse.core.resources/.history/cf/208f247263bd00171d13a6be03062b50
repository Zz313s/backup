		ziro.name = "木村次郎";
		ziro.age = 18;
		System.out.println(ziro.name);
		System.out.println(ziro.age);
		
		Person hanako = new Person();
		hanako.name = "鈴木花子";
		hanako.age = 16;
		System.out.println(hanako.name);
		System.out.println(hanako.age);
		
		
